#version 3.1;#include "colors.inc"#include "golds.inc"/* * A pocket computer on Edwin's desk. * scale is 1 POVunit == 1 inch. */global_settings {	assumed_gamma 1.8	ambient_light rgb<1, 1, 1>}camera {	location <0,46,0>	look_at <0,42,0>}light_source {	<0, 60, 0>	color White}plane { <0,1,0>, 0	pigment { Black }}// Edwin's desk#include "desk.inc"object { EdwinDesk }// The Mana Compass#include "palmcomp.inc"object {	Pocket_Comp	rotate <90,0,0>	rotate <0,-75,0>	translate <0,42.5,0>}