#version 3.1;#include "colors.inc"#include "golds.inc"/* * The mana compass on Edwin's desk. * scale is 1 POVunit == 1 inch. */global_settings {	assumed_gamma 1.8	ambient_light rgb<1, 1, 1>}camera {	location <3,46,-2.2>	look_at <0,42,0>}light_source {	<0, 60, 0>	color White}// Edwin's desk#include "desk.inc"object { EdwinDesk }// The Mana Compass#include "compass.inc"object {	Mana_Compass	translate <0,42.5,0>}